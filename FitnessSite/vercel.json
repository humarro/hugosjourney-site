{
  "redirects": [
    { "source": "/(.*)", "destination": "https://hugosjourney.com/$1", "permanent": true }
  ],
  "rewrites": [
    { "source": "/(.*)", "destination": "/" }
  ],
  "headers": [
    { "source": "/(.*)", "headers": [{ "key": "Strict-Transport-Security", "value": "max-age=63072000; includeSubDomains; preload" }] }
  ]
}
